echo $1        >  SESSION.NAME
echo `pwd`'/' >>  SESSION.NAME
touch $1.rea
rm ioinfo
rm *.lgl
cp $1.gph nekgraph
cp $1.map xxt_map.rea
mv $1.log.$2 $1.log1.$2
mv $1.fld $1.fld1
mv $1.his $1.his1
mv $1.sch $1.sch1
mv $1.out $1.out1
mv $1.ore $1.ore1
mv $1.nre $1.nre1
nohup mpirun -np $2  nek5000 > $1.log.$2 &
sleep 2
rm logfile
ln $1.log.$2 logfile
